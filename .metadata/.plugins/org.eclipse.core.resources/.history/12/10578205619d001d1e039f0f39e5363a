public class TC_CustomisedStatement_004 extends BaseClass {

	public void CustomisedStatement() throws InterruptedException {
		
		LoginPage lp = new LoginPage(driver);
		lp.setusername(user);
		lp.setpassword(pass);
		lp.clickLoginbtn();
		logger.info("login successful");
		Thread.sleep(3000);
		
	}
}